
a = int(input("Enter first value: "))
b = int(input("Enter second value: "))
c = int(input("Enter third value: "))

if(a <= b and a <= c):
    print(a, "is smaller")
elif(b <= a and b <= c):
    print(b, "is smaller")
else:
    print(c, "is smaller")

if(a == b and b == c):
    print(a, "is smaller")
elif(a == b and b > c):
    print(c, "is smaller")
elif(a == b and b < c):
    print(b, "is smaller")